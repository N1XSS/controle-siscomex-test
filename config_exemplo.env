# =============================================================================
# Configurações do Sistema de Controle DUE/Drawback
# =============================================================================
# Copie este arquivo para config.env e configure os valores reais
# NUNCA versione o arquivo config.env!

# Credenciais do Siscomex API (obrigatório)
SISCOMEX_CLIENT_ID=seu_client_id_aqui
SISCOMEX_CLIENT_SECRET=seu_client_secret_aqui

# PostgreSQL (obrigatório)
POSTGRES_HOST=seu_host_postgresql
POSTGRES_PORT=5432
POSTGRES_USER=seu_usuario
POSTGRES_PASSWORD=sua_senha
POSTGRES_DB=nome_do_banco

# AWS Athena (obrigatório)
AWS_ACCESS_KEY=seu_aws_access_key_aqui
AWS_SECRET_KEY=seu_aws_secret_key_aqui
AWS_REGION=us-east-1
ATHENA_CATALOG=AwsDataCatalog
ATHENA_DATABASE=default
ATHENA_WORKGROUP=primary
S3_OUTPUT_LOCATION=s3://locks-query-result/athena_odbc/
